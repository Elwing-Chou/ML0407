   "source": [
    "import pandas as pd\n",
    "from sklearn.datasets import load_iris\n",
    "iris = load_iris()\n",
    "df = pd.DataFrame(iris[\"data\"], \n",
    "                  columns=iris[\"feature_names\"])\n",
    "df[\"ans\"] = iris[\"target\"]\n",
    "df.to_csv(\"iris.csv\", \n",
    "          encoding=\"utf-8\",\n",
    "          index=False)\n",
    "df"
   ]
